generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id          Int @id @default(autoincrement())
  name        String
  password    String
  jwt         String
  Audios      Audio[]
}

model Audio {
  id       Int    @id @default(autoincrement())
  title    String
  path     String
  user_id  Int
  User     User   @relation(fields: [user_id], references: [id])
}